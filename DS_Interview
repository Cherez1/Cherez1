   "cell_type": "code",
   "execution_count": 13,
   "id": "f5dc3599",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2023-06-19T08:12:44.633171Z",
     "iopub.status.busy": "2023-06-19T08:12:44.632749Z",
     "iopub.status.idle": "2023-06-19T08:12:45.029772Z",
     "shell.execute_reply": "2023-06-19T08:12:45.028655Z"
    },
    "papermill": {
     "duration": 0.412624,
     "end_time": "2023-06-19T08:12:45.032440",
     "exception": false,
     "start_time": "2023-06-19T08:12:44.619816",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "def f(row):\n",
    "    if row['Day_of_Week'] == \"Thursday\":\n",
    "        val = date.strftime(date(2023, 6, 18), \"%d/%m/%Y\")\n",
    "    elif row['Day_of_Week'] == \"Friday\":\n",
    "        val = date.strftime(date(2023, 6, 19), \"%d/%m/%Y\")\n",
    "    else:\n",
    "        val = date.strftime(date(2023, 6, 20), \"%d/%m/%Y\")\n",
    "    return val\n",
    "\n",
    "#create new column 'Date' using the function above\n",
